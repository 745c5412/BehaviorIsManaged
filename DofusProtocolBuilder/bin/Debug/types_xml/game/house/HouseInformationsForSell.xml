<XmlType xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" Name="HouseInformationsForSell" Id="221" Heritage="Object">
	<Fields>
		<XmlField Name="modelId" Type="int" Condition="modelId &lt; 0" />
		<XmlField Name="ownerName" Type="utf" />
		<XmlField Name="ownerConnected" Type="boolean" />
		<XmlField Name="worldX" Type="short" Condition="worldX &lt; -255 || worldX &gt; 255" />
		<XmlField Name="worldY" Type="short" Condition="worldY &lt; -255 || worldY &gt; 255" />
		<XmlField Name="subAreaId" Type="short" Condition="subAreaId &lt; 0" />
		<XmlField Name="nbRoom" Type="sbyte" />
		<XmlField Name="nbChest" Type="sbyte" />
		<XmlField Name="skillListIds" Type="int[]" />
		<XmlField Name="isLocked" Type="boolean" />
		<XmlField Name="price" Type="int" Condition="price &lt; 0" />
	</Fields>
</XmlType>