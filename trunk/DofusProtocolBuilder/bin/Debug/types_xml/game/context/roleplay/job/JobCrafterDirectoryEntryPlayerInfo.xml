<XmlType xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" Name="JobCrafterDirectoryEntryPlayerInfo" Id="194" Heritage="Object">
	<Fields>
		<XmlField Name="playerId" Type="int" Condition="playerId &lt; 0" />
		<XmlField Name="playerName" Type="utf" />
		<XmlField Name="alignmentSide" Type="sbyte" />
		<XmlField Name="breed" Type="sbyte" Condition="breed &lt; (byte)Enums.PlayableBreedEnum.Feca || breed &gt; (byte)Enums.PlayableBreedEnum.Steamer" />
		<XmlField Name="sex" Type="boolean" />
		<XmlField Name="isInWorkshop" Type="boolean" />
		<XmlField Name="worldX" Type="short" Condition="worldX &lt; -255 || worldX &gt; 255" />
		<XmlField Name="worldY" Type="short" Condition="worldY &lt; -255 || worldY &gt; 255" />
		<XmlField Name="mapId" Type="int" />
		<XmlField Name="subAreaId" Type="short" Condition="subAreaId &lt; 0" />
	</Fields>
</XmlType>